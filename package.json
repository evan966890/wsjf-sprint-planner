{
  "name": "wsjf-sprint-planner",
  "version": "1.5.0",
  "description": "WSJF Sprint Planner - 基于WSJF方法的加权优先级排期可视化工具 (v1.5.0: 类型安全体系和调试方法论)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run ocr:server\"",
    "ocr:server": "node api/ocr-server.cjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-file-size": "node scripts/check-file-size.js",
    "verify-ocr": "node scripts/verify-ocr-integration.js",
    "pre-commit": "npm run check-file-size",
    "pre-deploy-check": "bash scripts/pre-deploy-check.sh",
    "deploy:tencent": "npm run pre-deploy-check && npm run build && cloudbase framework deploy",
    "deploy:vercel": "npm run pre-deploy-check && npm run build && vercel --prod",
    "prepare": "husky"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.309.0",
    "mammoth": "^1.11.0",
    "multer": "^1.4.5-lts.1",
    "pdfjs-dist": "^5.4.296",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^3.2.4"
  }
}
