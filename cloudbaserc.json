{
  "version": "2.0",
  "envId": "xiaomi-4g92opdf60df693e",
  "framework": {
    "name": "wsjf-sprint-planner",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "npm run build",
          "outputPath": "dist",
          "cloudPath": "/",
          "errorPath": "/index.html",
          "ignore": [
            ".git",
            ".github",
            "node_modules",
            ".gitignore"
          ]
        }
      },
      "feishu-proxy": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": "cloudbase-functions",
          "functions": [
            {
              "name": "feishu-proxy",
              "timeout": 60,
              "envVariables": {},
              "runtime": "Nodejs12.16",
              "memory": 256,
              "installDependency": true,
              "handler": "index.main"
            }
          ],
          "serviceName": "feishu-proxy",
          "serviceDescription": "飞书API代理服务"
        }
      }
    }
  }
}
